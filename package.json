    "axios": "^0.27.2",
    "element-plus": "^2.2.16",
    "mqtt": "^4.3.7",
    "pinia": "^2.0.21",
    "protobufjs": "^7.1.1",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "next"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "less": "^4.1.3",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.7",
    "vue-tsc": "^0.39.5"
  }
}
