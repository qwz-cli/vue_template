{
  "name": "template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "protoJs": "npx pbjs -t json-module -w commonjs -o src/utils/proto/proto.js src/utils/proto/*.proto",
